{
  "name": "mc-trip",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "start": "cross-env node ./dist/server.js",
    "build": "cross-env tsc -p tsconfig.prod.json && tsc-alias -p tsconfig.prod.json",
    "lint": "cross-env tsc --noEmit && eslint --ext .ts --ignore-path .gitignore \"src/**/*\"",
    "lint:fix": "cross-env tsc --noEmit && eslint --fix --ext .ts --ignore-path .gitignore \"src/**/*\"",
    "test": "cross-env DATABASE_URL=mysql://mctrip_user:mctrip_user_password@localhost:39011/mc_trip_test NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand ./src",
    "test:mac": "cross-env DATABASE_URL=mysql://mctrip_user:mctrip_user_password@localhost:39011/mc_trip_test NODE_ENV=test node --experimental-vm-modules ./node_modules/.bin/jest --runInBand ./src",
    "test_only": "cross-env DATABASE_URL=mysql://mctrip_user:mctrip_user_password@localhost:39011/mc_trip_test NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand",
    "test_only:mac": "cross-env DATABASE_URL=mysql://mctrip_user:mctrip_user_password@localhost:39011/mc_trip_test NODE_ENV=test node --experimental-vm-modules ./node_modules/.bin/jest --runInBand",
    "db_schema_validate": "cross-env prisma validate",
    "db_create_sql": "cross-env DATABASE_URL=mysql://root:password@localhost:39011/mc_trip_test prisma migrate dev --create-only --name=mc_trip",
    "db_migrate:develop": "cross-env DATABASE_URL=mysql://root:password@localhost:39011/mc_trip_test prisma migrate deploy",
    "db_migrate": "cross-env prisma migrate deploy",
    "db_migrate_reset:develop": "cross-env DATABASE_URL=mysql://root:password@localhost:39011/mc_trip_test prisma migrate reset",
    "db_reset": "cross-env docker-compose rm -fsv mysql && docker-compose up -d",
    "db_generate": "cross-env prisma generate",
    "openApi_build": "cross-env redocly build-docs ./docs/openApi/src/api.yaml -o ./docs/openApi/dist/index.html",
    "openApi_lint": "cross-env redocly lint ./docs/openApi/src/api.yaml",
    "openApi_lint:ignore_generate": "cross-env redocly lint ./docs/openApi/src/api.yaml --generate-ignore-file",
    "batch": "cross-env NODE_ENV=test node ./dist/jobs/cycleAfterApplyTestCommand.js"
  },
  "lint-staged": {
    "*.{ts,js}": "yarn lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@azure/storage-blob": "^12.16.0",
    "@prisma/client": "^5.7.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "archiver": "^6.0.1",
    "axios": "^1.4.0",
    "basic-auth": "^2.0.1",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "croner": "^7.0.5",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "4.18.2",
    "fast-xml-parser": "^4.3.2",
    "helmet": "^7.0.0",
    "https-proxy-agent": "^7.0.2",
    "jsdom": "^23.0.1",
    "jsonwebtoken": "^9.0.1",
    "lodash-es": "^4.17.21",
    "log4js": "^6.9.1",
    "nodemailer": "^6.9.8",
    "papaparse": "^5.4.1",
    "qs": "^6.11.2",
    "uuid": "^9.0.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@redocly/cli": "^1.0.0-rc.3",
    "@types/archiver": "^6.0.2",
    "@types/basic-auth": "^1.1.4",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsdom": "^21.1.6",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.4.2",
    "@types/nodemailer": "^6.4.14",
    "@types/papaparse": "^5.3.13",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.3",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "cross-env": "^7.0.3",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "prisma": "^5.7.1",
    "prisma-erd-generator": "^1.11.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}
